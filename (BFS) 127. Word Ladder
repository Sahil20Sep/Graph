class Solution {
public:
    int ladderLength(string s, string t, vector<string>& v) {
        map<string, bool> hash;
        int n = v.size();
        for(int i = 0; i < n; i++){
            hash[v[i]] = true;
        }
        if(!hash[t] || s.size() != t.size()){
            return 0;
        }
        queue<pair<string, int>> Q;
        Q.push({s, 1});
        while(!Q.empty()){
            auto f = Q.front();
            string a = f.first;
            int b = f.second;
            if(a == t){
                return b;
            }
            Q.pop();
            for(int i = 0; i < a.size(); i++){
                string temp = a;
                for(int j = 0; j < 26; j++){
                    temp[i] = char('a' + j);
                    if(hash[temp]){
                        hash[temp] = false;
                        Q.push({temp, b + 1});
                    }
                }
            }
        }
        return 0;
    }
};
